{
  "name": "tamagui",
  "version": "1.142.0",
  "type": "module",
  "description": "Style and UI for React (web and native) meet an optimizing compiler",
  "removeSideEffects": true,
  "alsoPublishAs_disabled": [
    "@tamagui/ui"
  ],
  "sideEffects": [
    "setup.js"
  ],
  "source": "src/index.ts",
  "types": "./types/index.d.ts",
  "main": "dist/cjs",
  "module": "dist/esm",
  "module:jsx": "dist/jsx",
  "files": [
    "src",
    "types",
    "dist",
    "linear-gradient",
    "native.js",
    "native.d.ts"
  ],
  "scripts": {
    "build": "tamagui-build",
    "afterBuild": "node bundle-native.mjs",
    "watch": "tamagui-build --watch",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "check": "yarn check-circular-deps",
    "check-circular-deps": "npx madge --circular ./src/index.ts"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "react-native": {
        "module": "./dist/esm/index.native.js",
        "import": "./dist/esm/index.native.js",
        "require": "./dist/cjs/index.native.js"
      },
      "types": "./types/index.d.ts",
      "module": "./dist/esm/index.mjs",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/cjs/index.native.js"
    },
    "./react-native-web": {
      "react-native": "./dist/cjs/react-native-web.native.js",
      "types": "./types/index.d.ts",
      "module": "./dist/esm/react-native-web.mjs",
      "import": "./dist/esm/react-native-web.mjs",
      "require": "./dist/cjs/react-native-web.cjs"
    },
    "./web": {
      "react-native": {
        "module": "./dist/esm/index.native.js",
        "import": "./dist/esm/index.native.js",
        "require": "./dist/cjs/index.native.js"
      },
      "types": "./types/index.d.ts",
      "module": "./dist/esm/index.mjs",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    },
    "./native": {
      "types": "./types/index.d.ts",
      "require": "./dist/native.cjs"
    },
    "./native-test": {
      "types": "./types/index.d.ts",
      "require": "./dist/test.cjs",
      "react-native": "./dist/test.native.js"
    },
    "./linear-gradient": {
      "react-native": {
        "module": "./dist/esm/linear-gradient.native.js",
        "import": "./dist/esm/linear-gradient.native.js",
        "require": "./dist/cjs/linear-gradient.native.js"
      },
      "types": "./types/linear-gradient.d.ts",
      "module": "./dist/esm/linear-gradient.mjs",
      "import": "./dist/esm/linear-gradient.mjs",
      "require": "./dist/cjs/linear-gradient.cjs"
    }
  },
  "dependencies": {
    "@tamagui/accordion": "1.142.0",
    "@tamagui/adapt": "1.142.0",
    "@tamagui/alert-dialog": "1.142.0",
    "@tamagui/animate-presence": "1.142.0",
    "@tamagui/avatar": "1.142.0",
    "@tamagui/button": "1.142.0",
    "@tamagui/card": "1.142.0",
    "@tamagui/checkbox": "1.142.0",
    "@tamagui/compose-refs": "1.142.0",
    "@tamagui/constants": "1.142.0",
    "@tamagui/core": "1.142.0",
    "@tamagui/create-context": "1.142.0",
    "@tamagui/dialog": "1.142.0",
    "@tamagui/elements": "1.142.0",
    "@tamagui/fake-react-native": "1.142.0",
    "@tamagui/focusable": "1.142.0",
    "@tamagui/font-size": "1.142.0",
    "@tamagui/form": "1.142.0",
    "@tamagui/get-button-sized": "1.142.0",
    "@tamagui/get-font-sized": "1.142.0",
    "@tamagui/get-token": "1.142.0",
    "@tamagui/group": "1.142.0",
    "@tamagui/helpers-tamagui": "1.142.0",
    "@tamagui/image": "1.142.0",
    "@tamagui/input": "1.142.0",
    "@tamagui/label": "1.142.0",
    "@tamagui/linear-gradient": "1.142.0",
    "@tamagui/list-item": "1.142.0",
    "@tamagui/polyfill-dev": "1.142.0",
    "@tamagui/popover": "1.142.0",
    "@tamagui/popper": "1.142.0",
    "@tamagui/portal": "1.142.0",
    "@tamagui/progress": "1.142.0",
    "@tamagui/radio-group": "1.142.0",
    "@tamagui/react-native-media-driver": "1.142.0",
    "@tamagui/scroll-view": "1.142.0",
    "@tamagui/select": "1.142.0",
    "@tamagui/separator": "1.142.0",
    "@tamagui/shapes": "1.142.0",
    "@tamagui/sheet": "1.142.0",
    "@tamagui/slider": "1.142.0",
    "@tamagui/stacks": "1.142.0",
    "@tamagui/switch": "1.142.0",
    "@tamagui/tabs": "1.142.0",
    "@tamagui/text": "1.142.0",
    "@tamagui/theme": "1.142.0",
    "@tamagui/toggle-group": "1.142.0",
    "@tamagui/tooltip": "1.142.0",
    "@tamagui/use-controllable-state": "1.142.0",
    "@tamagui/use-debounce": "1.142.0",
    "@tamagui/use-force-update": "1.142.0",
    "@tamagui/use-window-dimensions": "1.142.0",
    "@tamagui/visually-hidden": "1.142.0",
    "@tamagui/z-index-stack": "1.142.0"
  },
  "devDependencies": {
    "@tamagui/build": "1.142.0",
    "react": "*",
    "react-native": "0.81.5",
    "react-native-web": "^0.21.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tamagui/tamagui.git",
    "directory": "code/ui/tamagui"
  },
  "gitHead": "a49cc7ea6b93ba384e77a4880ae48ac4a5635c14",
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  }
}