{
  "expo": {
    "name": "fyt",
    "slug": "fyt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "fyt",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.it.fyt",
      "infoPlist": {
        "NSCameraUsageDescription": "fyt needs camera access to record workout proof videos.",
        "NSPhotoLibraryUsageDescription": "fyt needs photo library access to upload workout proof.",
        "NSContactsUsageDescription": "fyt needs contacts access to invite friends to challenges."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.it.fyt",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_CONTACTS",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.VIBRATE"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow fyt to access your camera for workout proof."
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow fyt to access your contacts to invite friends."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#10B981"
        }
      ],
      [
        "@sentry/react-native/expo",
        {
          "organization": "fyt-apps",
          "project": "fyt-mobile"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "037f5c40-a3a5-4088-979a-522d1aa24c7c"
      }
    },
    "owner": "fyt-apps"
  }
}
