# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_PUBLISHABLE_KEY=your-anon-key

# Expo/EAS Project ID (from expo.dev)
EXPO_PUBLIC_PROJECT_ID=your-eas-project-id

# Sentry Error Monitoring
# Get your DSN from https://sentry.io -> Project Settings -> Client Keys
EXPO_PUBLIC_SENTRY_DSN=https://your-key@sentry.io/your-project-id

# PostHog Analytics
# Get your API key from https://posthog.com -> Project Settings
EXPO_PUBLIC_POSTHOG_API_KEY=phc_your-api-key
EXPO_PUBLIC_POSTHOG_HOST=https://us.i.posthog.com
